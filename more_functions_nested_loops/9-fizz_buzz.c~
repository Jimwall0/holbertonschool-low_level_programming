#include <stdio.h>
/**
 * main - for the code to be placed
 * Return: 0
 */
int main(void)
{
for (int num = 1; num <= 100; num ++)
{
if (num % 3 == 0)
{
printf("Fizz\n");
}
else if (num % 5 == 0)
{
printf("Buzz\n");
}
else if (num % 3 == 0 && num % 5 == 0)
{
printf("FizzBuzz\n");
}
else
{
printf('0' + "%d", num);
}
}
}
