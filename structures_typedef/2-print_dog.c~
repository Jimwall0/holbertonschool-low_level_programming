#include "dog.h"
#include <stdio.h>
/**
 * print_dog - prints a do
 * @d: structure we are useing
 */
void print_dog(struct dog *d)
{
  if (d != NULL)
    {
      if (d->name == NULL)
	{
	  printf("(nil)");
	}
      else
	{
	  printf("%s", d->name);
	}
      if (d->age)
	{
	  printf("(nil)");
	}
      else
	{
	  printf("%f", d->age);
	}
      if (d->owner == NULL)
	{
	  printf("(nil)");
	}
      else
	{
	  printf("%s", d->owner);
	}
    }
}
