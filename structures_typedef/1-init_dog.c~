#include "dog.h"
/**
 * init_dog - initialize a variable of type struct dog
 * @d: struct dog
 * @name: name of dog
 * @age: how old the dog is
 * @owner: who owns the dog
 */
void init_dog(struct dog *d, char *name, float age, char *owner)
{
struct dog dog;
dog.name -> name;
dog.age -> age;
dog.owner -> owner;
}
